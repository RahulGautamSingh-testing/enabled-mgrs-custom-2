{
  "enabledManagers": [
    "custom.regex",
    "nvm",
    "npm"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "index.ts"
      ],
      "matchStrings": [
        "default: '(?<depName>ghcr.io/containerbase/sidecar):(?<currentValue>.*)'"
      ],
      "datasourceTemplate": "docker"
    }
  ],
  "prHourlyLimit": 99,
  "configMigration": true
}
